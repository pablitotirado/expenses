{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend NestJS con infraestructura como c√≥digo usando Pulumi",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:swc": "swc src -d dist --config-file .swcrc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "./scripts/start-local.sh",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:run": "vitest run",
    "test:dev": "vitest --ui --open",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:status": "prisma migrate status",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "infra:build": "tsc -p infra/tsconfig.json",
    "infra:preview": "cd infra && pulumi preview",
    "infra:deploy": "cd infra && pulumi up",
    "infra:destroy": "cd infra && pulumi destroy",
    "infra:refresh": "cd infra && pulumi refresh",
    "infra:stack": "cd infra && pulumi stack",
    "docker:build:local": "docker build -t expense-manager-backend:local .",
    "docker:run:local": "docker run -p 3000:3000 expense-manager-backend:local"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.879.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.15.0",
    "expenses-shared": "workspace:^",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "openai": "^5.19.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@pulumi/aws": "^6.0.0",
    "@pulumi/docker": "^4.0.0",
    "@pulumi/pulumi": "^3.192.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  }
}
